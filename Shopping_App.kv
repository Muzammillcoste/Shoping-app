ScreenManager:
    LoginWindow:
    SignupWindow:
    MainWindow:
    GrossWindow:
    ElectWindow:
    FashionWindow:
    PaymentWindow:

<SignupWindow>:
    name:'Signup'
    Image:
        source:'blue.png'
        allow_stretch:True
        keep_ratio:False
    MDCard:
        size_hint:None,None
        size:300,525
        pos_hint:{'center_x':0.5,'center_y':0.5}
        elevation:10
        padding:25
        spacing:25
        orientation:'vertical'
        MDLabel:
            text:"'WALMART' Signup"
            font_size:40
            font_name:'Auto Rich.ttf'  
            halign:'center'
            size_hint_y:None   
            height:self.texture_size[1]
        MDTextField:
            id:Name
            hint_text:'Name'
            helper_text_mode:'on_focus'
            icon_right_color: app.theme_cls.primary_color
            icon_right:'account'
            size_hint:None,None
            multiline: False
            size:'250dp','30dp'
            pos:root.width/3,root.height/1.75
        MDTextField:
            id:Email
            hint_text:'Email'
            helper_text_mode:'on_focus'
            icon_right_color: app.theme_cls.primary_color
            text_hint:'Email'
            icon_right:'email'
            size_hint:None,None
            multiline: False
            size:'250dp','30dp'
            pos:root.width/3,root.height/2
        MDTextField:
            id:Password
            hint_text:'Password'
            helper_text_mode:'on_focus'
            icon_right_color: app.theme_cls.primary_color
            icon_right:'eye-off'
            size_hint:None,None
            multiline: False
            size:'250dp','30dp'
            password:True
            pos:root.width/3,root.height/2.3
        MDTextField:
            id:Phone_Number
            hint_text:'Phone number'
            helper_text_mode:'on_focus'
            icon_right_color: app.theme_cls.primary_color
            icon_right:'phone'
            size_hint:None,None
            multiline: False
            size:'250dp','30dp'
            pos:root.width/3,root.height/2.8
        MDRoundFlatButton:
            text: 'Login'
            font_size:18
            pos_hint:{'center_x':0.5}
            size:'100dp','50dp'
            on_release:
                app.root.current='Login'
                root.manager.transition.direction='right'
        MDRoundFlatButton:
            text: 'Submit'
            font_size:18
            pos_hint:{'center_x':0.5}
            size:'100dp','50dp'
            on_release:
                app.root.current='Signup'
                root.manager.transition.direction='right'
                root.Submit_signup_func(Name.text,Email.text,Password.text,Phone_Number.text)

<LoginWindow>:
    name:'Login'
    Image:
        source:'blue.png'
        allow_stretch:True
        keep_ratio:False
        cols: 1
        MDToolbar:
            title: "Login"
            left_action_items: [["menu",lambda x: app.main_menu_dialog(Email.text,Password.text)]]
    MDCard:
        size_hint:None,None
        size:300,380
        pos_hint:{'center_x':0.5,'center_y':0.5}
        elevation:10
        padding:25
        spacing:25
        orientation:'vertical'
        MDLabel:
            text:"'WALMART' Login"
            font_size:40
            font_name:'Auto Rich.ttf'  
            halign:'center'
            size_hint_y:None   
            height:self.texture_size[1]
        MDTextField:
            id:Email
            hint_text:'Email'
            helper_text_mode:'on_focus'
            icon_right_color: app.theme_cls.primary_color
            icon_right:'email'
            size_hint:None,None
            icon_right_color: app.theme_cls.primary_color
            multiline: False
            size:'250dp','30dp'
            pos:root.width/3,root.height/2
            pos:root.width/3,root.height/2
        MDTextField:
            id:Password
            hint_text:'Password'
            helper_text_mode:'on_focus'
            icon_right:'eye-off'
            size_hint:None,None
            icon_right_color: app.theme_cls.primary_color
            multiline: False
            size:'250dp','30dp'
            password:True
            pos:root.width/3,root.height/2.3
        MDRoundFlatButton:
            text: 'Login'
            font_size:18
            pos_hint:{'center_x':0.5}
            size:'100dp','50dp'
            on_state:
                if bool(root.Log_in_func(Email.text,Password.text)):\
                root.manager.transition.direction='right';\
                app.root.current="Main"
        MDRoundFlatButton:
            text: 'Signup'
            font_size:18
            pos_hint:{'center_x':0.5}
            size:'100dp','50dp'
            on_release:
                app.root.current='Signup'
                root.manager.transition.direction='right'

        
<MainWindow>:
    name:'Main'
    Image:
        source:'blue.png'
        allow_stretch:True
        keep_ratio:False
    MDGridLayout:

        BoxLayout:
            color:0,0,1,1
            orientation:'horizontal'
            spacing:'30dp'
            padding:'230dp'
            MDFillRoundFlatButton:
                text: 'Grossery'
                font_size:18    
                on_release:
                    app.root.current='Gross'
                    root.manager.transition.direction='right'
                color:(0,0,1,.5)
            MDFillRoundFlatButton:
                text: 'Electronics'
                font_size:18
                on_release:
                    app.root.current='Elect'
                    root.manager.transition.direction='right'
            MDFillRoundFlatButton:
                text: 'Fashion'
                font_size:18
                on_release:
                    app.root.current='Fashion'
                    root.manager.transition.direction='right'

